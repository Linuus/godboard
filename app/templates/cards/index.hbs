<div class="panel panel-default swimlane">
  <div class="panel-heading">
    <h5 class="panel-title">Icebox</h5>
  </div>
  <div class="panel-body">
    {{#each}}
      <div class="panel panel-default card">
        <div class="panel-body">
          <a href="#" {{action 'deleteCard' this}} class="delete-button">&times;</a>
          {{body}}
        </div>
      </div>
    {{/each}}
  </div>
  <div class="panel-footer">
    {{ render 'cards/new' }}
  </div>
</div>
